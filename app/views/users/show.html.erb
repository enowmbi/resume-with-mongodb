<main>
  <h2 class="title"> Account Profile(<%= "#{current_user.username}" %>)</h2>
  <div id="<%= dom_id current_user %>">
    <p>
    <div class="user-details">
      <%= render "shared/user_details" %>
    </div>
    <div>
      <%= render "shared/address_details" %>
    </div>
    <hr/>
    </p>
    <h3>Profile</h3>
    <div id="new-profile-form">
      <%= render "profiles/form", profile: @profile %>
    </div>
    <div id="profile-entries">
      <%# <% current_user.profile.links.each do |link| %1> %>
        <%# <%= render "profiles/link", link: link %1> %>
      <%# <% end %1> %>
    </div>
    <hr/>
    <h3>Skill set </h3>
    <div id="new-skill-set-form">
      <%= render "skill_sets/form", skill_set: @skill_set %>
    </div>
    <div id="skill_sets">
      <% current_user.skill_sets.each do |skill_set| %>
        <%= render "skill_sets/skill_set", skill_set: skill_set %>
      <% end %>
    </div>
    <hr/>
  </div>  
  <p>
  <%= link_to("Edit Account", edit_user_registration_path, class: "btn-square-less-space") %>
  </p>
  <hr/>
  <%= link_to("Back", :back) %>
</main>
